<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="20dp"
    android:layout_marginLeft="8dp"
    android:layout_marginRight="8dp"
    android:layout_marginTop="20dp"
    android:background="@color/appWhite"
    android:clickable="false"
    app:cardCornerRadius="8dp"
    app:cardElevation="7dp">

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/pinUnpinButton"
        android:layout_width="60dp"
        android:layout_height="36dp"
        android:layout_marginEnd="8dp"
        android:background="@drawable/outline_button_reject"
        android:elevation="0dp"
        android:fontFamily="@font/open_sans_bold"
        android:padding="0dp"
        android:selectAllOnFocus="true"
        android:text="Unpin"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@color/appReject"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@id/detailsButton"
        app:layout_constraintTop_toTopOf="@id/detailsButton"/>

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/detailsButton"
        android:layout_width="60dp"
        android:layout_height="36dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/outline_button"
        android:elevation="0dp"
        android:fontFamily="@font/open_sans_bold"
        android:padding="0dp"
        android:selectAllOnFocus="true"
        android:text="Details"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@color/appGreen"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <android.support.v7.widget.AppCompatTextView
        android:id="@+id/hostname"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:fontFamily="@font/open_sans_bold"
        android:text="github.com"
        android:textColor="@color/appBlack"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@id/detailsButton"
        app:layout_constraintEnd_toStartOf="@+id/pinUnpinButton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/detailsButton"/>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/detailsContainer"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/detailsButton">

        <android.support.v7.widget.AppCompatButton
            android:id="@+id/copyButton"
            android:layout_width="60dp"
            android:layout_height="36dp"
            android:layout_marginBottom="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/outline_button"
            android:elevation="0dp"
            android:fontFamily="@font/open_sans_bold"
            android:padding="0dp"
            android:selectAllOnFocus="true"
            android:text="Copy"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textColor="@color/appGreen"
            android:textSize="12sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/fullKey"/>

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/fullKey"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:fontFamily="@font/code"
            android:text=" "
            android:textAlignment="viewStart"
            android:textColor="@color/appBlack"
            android:textSize="6sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/memberEmail8" />

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/memberEmail8"
            android:layout_width="0dp"
            android:layout_height="17dp"
            android:layout_marginTop="16dp"
            android:fontFamily="@font/open_sans_bold"
            android:text="PUBLIC KEY"
            android:textColor="@color/appGray"
            android:textSize="10sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/keyFingerprint" />

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/keyFingerprint"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/code_bold"
            android:singleLine="true"
            android:text="SHA256:Xc4MD22hj1ZwA9bSSZE1wB05yTRnpVtxuWQI3fyT/jc= "
            android:textColor="@color/appBlack"
            android:textSize="10sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/memberEmail5"
            app:layout_constraintTop_toBottomOf="@+id/memberEmail5"/>

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/memberEmail5"
            android:layout_width="0dp"
            android:layout_height="17dp"
            android:fontFamily="@font/open_sans_bold"
            android:text="FINGERPRINT"
            android:textColor="@color/appGray"
            android:textSize="10sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </android.support.constraint.ConstraintLayout>

</android.support.constraint.ConstraintLayout>
